{
    "name": "draftscripts/permission",
    "description": "Laravel Permission",
    "type": "library",
    "require": {
        "spatie/laravel-permission": "^6.16",
        "livewire/livewire": "^3.6",
        "maatwebsite/excel": "^3.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DraftScripts\\Permission\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DraftScripts\\Permission\\PermissionServiceProvider"
            ],
            "aliases": {
                "LaraPermission": "DraftScripts\\Permission\\Facades\\LaraPermission"
            }
        }
    },
    "minimum-stability": "stable"
}
